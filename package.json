{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "dev": "npm run build:content && vite",
    "precompile-mdx": "tsx scripts/precompile-mdx.ts",
    "precompile-kanban": "node scripts/precompile-kanban.js",
    "kanban": "node scripts/kanban-cli.js",
    "kanban:add": "node scripts/kanban-cli.js add",
    "kanban:move": "node scripts/kanban-cli.js move",
    "kanban:list": "node scripts/kanban-cli.js list",
    "kanban:sync": "node scripts/kanban-cli.js sync",
    "copy-metrics": "node scripts/copy-metrics-to-public.js",
    "build:content": "node scripts/copy-metrics-to-public.js && tsx scripts/precompile-mdx.ts && node scripts/precompile-kanban.js && tsx scripts/generate-feeds.ts && node scripts/validate-projects.mjs && node scripts/generate-og-images.mjs",
    "build:vite": "vite build",
    "build": "npm run build:content && npm run build:vite && node scripts/prerender.mjs",
    "build:netlify": "npm run build:content && npm run build:vite",
    "build:preview": "npm run build:content && npm run build:vite",
    "build:dev": "tsx scripts/precompile-mdx.ts && node scripts/precompile-kanban.js && tsx scripts/generate-feeds.ts && vite build --mode development",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prerender": "node scripts/prerender.mjs",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "generate-feeds": "tsx scripts/generate-feeds.ts",
    "check-seo": "tsx scripts/check-seo-metrics.ts",
    "lighthouse": "lighthouse http://localhost:8080 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "lighthouse:production": "lighthouse https://dylanbochman.com --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "save-lighthouse-scores": "node scripts/save-lighthouse-scores.js",
    "generate-metrics-summary": "node scripts/generate-metrics-summary.js",
    "fetch-search-console": "node scripts/fetch-search-console-data.js",
    "fetch-ga4": "node scripts/fetch-ga4-data.js",
    "prepare": "husky",
    "analyze": "ANALYZE=true npm run build && echo 'Bundle analysis at dist/bundle-analysis.html'"
  },
